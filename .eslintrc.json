{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "allowAfterThisConstructor": true,
        "no-console": "off",
        "indent": ["error", 4],
        "prettier/prettier": ["error"]
    }
}
